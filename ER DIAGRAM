	CUSTOMER
	
cust_id	PRIMARY KEY		// 
name
phone
email
mpin
acc_no				// bank_id + branch_id + cust_id
balance
bank_id FOREIGN KEY

	TRANSACTIONS
	
trans_id	PRIMARY KEY
sender_id
reciever_id
date
time
amount

	BANK
	
branch_id		PRIMARY KEY		// City_Abbrevation
branch			// City
bank_id			// Abbrevation
bank_name		// Full Form

	LOAN
	
loan_id		PRIMARY KEY
bank_id		FOREIGN KEY referencing BANK
cust_id		FOREIGN KEY referencing CUSTOMER
amount
emi
deadline

	DEFALUTER
	
cust_id		PRIMARY KEY | FOREIGN KEY referencing CUSTOMER
due
penalty
