
	CUSTOMER
	
cust_id	PRIMARY KEY
name
phone
email
mpin
balance

	TRANSACTIONS
	
trans_id	PRIMARY KEY
from_id
to_id
date
time
amount

	BANK
	
branch_id		PRIMARY KEY
branch_name
bank_id		
bank_name

	LOAN
	
loan_id		PRIMARY KEY
bank_id		FOREIGN KEY referencing BANK
cust_id		FOREIGN KEY referencing CUSTOMER
amount
emi
deadline

	DEFALUTER
	
cust_id		PRIMARY KEY | FOREIGN KEY referencing CUSTOMER
due
penalty
